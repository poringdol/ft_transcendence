<div class = "message mb-2">
  <div class = "content-container">
    <div class = "messageContent">
      <%= message.content %>
    </div>
    <div class = "author">
      <%= message.user.nickname %> (<%= Guild.all.find(message.user.guild_id).anagram if message.user.guild_id? %>)
    </div>
  </div>
</div>
